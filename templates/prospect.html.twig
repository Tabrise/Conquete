{% extends 'base.html.twig' %}

{% block title %}Mon Prospect
{% endblock %}

{% block body%}
	<div class="card text-center">
		{{include('societe/_show.html.twig')}}
		<div class="card-body">
			<div class="tab-content">
				<div class="tab-pane fade show active" id="contact">
					<p class="card-text">{{render(path('app_contact',{s:prospect}))}}</p>
				</div>
				<div class="tab-pane fade" id="audit">
					<p class="card-text">{{render(path('app_audit',{s:prospect}))}}</p>
				</div>
			</div>
		</div>
	</div>
	<a href="{{ path('app_societe') }}" class="btn btn-primary" style="color:white">
		<i class="bi bi-arrow-left"></i>
		Retour
	</a>
	{{render(path('app_modifier_societe',{s:prospect}))}}
{% endblock %}